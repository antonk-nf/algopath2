# Environment Configuration Example
# Copy this file to .env and adjust values as needed

# Application Environment
ENVIRONMENT=development

# Data Configuration
DATA_ROOT_PATH=.
CACHE_DIR=.cache

# API Configuration
API_HOST=127.0.0.1
API_PORT=8000
API_WORKERS=1

# Database Configuration (Optional)
DATABASE_URL=sqlite:///leetcode_analytics.db
DATABASE_ENABLED=false

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL_HOURS=24

# Processing Configuration
PARALLEL_WORKERS=4
CHUNK_SIZE=10000

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json